{
  "name": "lynda-video-transcripts",
  "version": "0.0.1",
  "description": "自动抓取 Lynda 视频声音注解的爬虫脚本",
  "scripts": {
    "build": "NODE_ENV=production node worker",
    "dev": "node worker",
    "lint": "eslint . && echo \"ESLint passed ✔\"",
    "start": "npm run build && node init.js && phantomjs --load-images=false --config=phantomjs/config.json lib/entry.js",
    "test": "node lib/test"
  },
  "author": "Riophae Lee <riophaelee@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "lodash": "^3.10.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.6",
    "rimraf": "^2.4.4",
    "sanitize-filename": "^1.6.1",
    "timer-stopwatch": "^0.1.6",
    "yaku": "^0.11.4"
  },
  "devDependencies": {
    "babel-eslint": "^4.1.6",
    "babel-plugin-add-module-exports": "^0.1.1",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-stage-0": "^6.1.18",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.2",
    "eslint-config-airbnb": "^2.0.0",
    "npmlog": "^2.0.0",
    "pre-push": "^0.1.1",
    "watchify": "^3.6.1",
    "yamlify": "^0.1.2"
  },
  "pre-push": [
    "lint"
  ]
}
